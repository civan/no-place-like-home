{
    "workbench.settings.editor": "json",
    "vim.easymotion": true,
    "vim.surround": true,
    "vim.camelCaseMotion.enable": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": false,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.foldfix": true,
    "vim.insertModeKeyBindings": [
      {
        "before": ["j", "j"],
        "after": ["<Esc>"]
      }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": ["<leader>", "d"],
        "after": ["d", "d"]
      },
      {
        "before": ["<leader>", "y"],
        "after": ["\"", "*", "y"]
      },
      {
        "before": ["<leader>", "p"],
        "after": ["\"", "*", "p"]
      },
      {
        "before": ["<leader>", "r"],
        "commands": [
          "workbench.view.explorer",
          "workbench.files.action.showActiveFileInExplorer"
        ]
      },
      {
        "before": ["<leader>", "o"],
        "commands": [
            "workbench.action.quickOpen"
        ]
      },
      {
        "before": ["<C-l>"],
        "commands": [
          "workbench.action.focusNextGroup"
        ]
      },
      {
        "before": ["<C-h>"],
        "commands": [
          "workbench.action.focusPreviousGroup"
        ]
      },
      {
        "before": ["g", "r"],
        "commands": [
          "editor.action.rename"
        ]
      }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
      "<C-a>": false,
      "<C-f>": false
    },
    "cSpell.userWords": [
      "Buganizer",
      "Gerrit",
      "ngrx",
      "nocheck",
      "objectcache",
      "promisify",
      "susy",
      "tweenmax",
      "webp"
    ],
    "workbench.iconTheme": "vscode-icons",
    "yaml.customTags": [
      "!g.yaml",
      "!g.doc",
      "!g.url"
    ],
    "editor.lineNumbers": "relative",
    "[scss]": {
      "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[html]": {
      "editor.defaultFormatter": "HookyQR.beautify"
    },
    "files.associations": {
      "*.tpl": "jinja-html"
    },
    "[json]": {
      "editor.defaultFormatter": "HookyQR.beautify"
    },
    "editor.rulers": [
      { "column": 80, "color": "#584858" },
      { "column": 120, "color": "#490808" }
    ],
    "[jinja-html]": {},
  "window.zoomLevel": 0,
  "cSpell.enableFiletypes": [
    "ssh_config"
  ],
  "markdown.previewFeatures": true,
  "markdown.preview.breaks": false,
  "markdown.styles": [
   // "github-markdown.css"
  ],
  "markdownlint.config": {
    "default": true,
    "MD012": false,
    "MD004": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "workbench.editor.showTabs": true
}
