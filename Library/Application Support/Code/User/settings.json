{
  "workbench.settings.editor": "json",
  "vim.easymotion": true,
  "vim.surround": true,
  "vim.camelCaseMotion.enable": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": false,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.foldfix": true,
  "trailing-spaces.highlightCurrentLine": false,
  "editor.renderWhitespace": "selection",
  "editor.formatOnSave": true,
  "editor.find.cursorMoveOnType": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "y"
      ],
      "after": [
        "\"",
        "*",
        "y"
      ]
    },
    {
      "before": [
        "<leader>",
        "p"
      ],
      "after": [
        "\"",
        "*",
        "p"
      ]
    },
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "c"
      ],
      "commands": [
        "editor.action.dirtydiff.next"
      ]
    },
    {
      "before": [
        "gn"
      ],
      "commands": [
        "editor.action.dirtydiff.next"
      ],
      "when": "dirtyDiffVisible"
    },
    {
      "before": [
        "gN"
      ],
      "commands": [
        "editor.action.dirtydiff.previous"
      ],
      "when": "dirtyDiffVisible"
    },
    {
      "before": [
        "<leader>",
        "d"
      ],
      "after": [
        "d",
        "d"
      ]
    },
    {
      "before": [
        "<leader>",
        "y"
      ],
      "after": [
        "\"",
        "*",
        "y"
      ]
    },
    {
      "before": [
        "<leader>",
        "p"
      ],
      "after": [
        "\"",
        "*",
        "p"
      ]
    },
    {
      "before": [
        "<leader>",
        "r"
      ],
      "commands": [
        "workbench.view.explorer",
        "workbench.files.action.showActiveFileInExplorer"
      ]
    },
    {
      "before": [
        "<leader>",
        "o"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<C-l>"
      ],
      "commands": [
        "workbench.action.focusNextGroup"
      ]
    },
    {
      "before": [
        "<C-h>"
      ],
      "commands": [
        "workbench.action.focusPreviousGroup"
      ]
    },
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "editor.action.rename"
      ]
    },
    {
      "before": [
        "g",
        "o"
      ],
      "commands": [
        "editor.action.openLink"
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "cSpell.userWords": [
    "Buganizer",
    "Devs",
    "GLTF",
    "Gerrit",
    "Lpaas",
    "OKRs",
    "drei",
    "gltfs",
    "gsap",
    "ngrx",
    "nocheck",
    "objectcache",
    "plentycode",
    "promisify",
    "scroller",
    "susy",
    "tweenmax",
    "upto",
    "webp"
  ],
  "workbench.iconTheme": "material-icon-theme",
  "yaml.customTags": [
    "!g.yaml",
    "!g.doc",
    "!g.url"
  ],
  "editor.lineNumbers": "relative",
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "files.associations": {
    "*.tpl": "jinja-html",
    ".firebaserc": "json"
  },
  "[json]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "editor.rulers": [
    {
      "column": 80,
      "color": "#584858"
    },
    {
      "column": 120,
      "color": "#490808"
    }
  ],
  "[jinja-html]": {},
  "cSpell.enableFiletypes": [
    "ssh_config"
  ],
  "markdown.previewFeatures": true,
  "markdown.preview.breaks": false,
  "markdown.styles": [
    // "github-markdown.css"
  ],
  "markdownlint.config": {
    "default": true,
    "MD012": false,
    "MD004": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.showTabs": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.experimentalLinkProvider": true,
  "terminal.integrated.experimentalUseTitleEvent": true,
  "editor.suggest.statusBar.visible": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": [
    "#005f5f"
  ],
  "vim.statusBarColors.insert": "#490000",
  "vim.statusBarColors.visual": "#003364",
  "vim.statusBarColors.visualline": "#003364",
  "vim.statusBarColors.visualblock": "#001937",
  "vim.statusBarColors.replace": "#D08770",
  "vim.statusBarColors.commandlineinprogress": "#411c11",
  "vim.statusBarColors.searchinprogressmode": "#6f4311",
  "vim.statusBarColors.easymotionmode": "#00ff00",
  "vim.statusBarColors.easymotioninputmode": "#ff0000",
  "vim.statusBarColors.surroundinputmode": "#392855",
  "files.exclude": {
    "**/.DS_Store": true
  },
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f",
    "activityBar.background": "#0d1d31",
    "titleBar.activeBackground": "#1f2435",
    "titleBar.activeForeground": "#F2FEF4",
    "titleBar.inactiveForeground": "#ffffffcc",
    "titleBar.inactiveBackground": "#FF2C70CC"
  },
  "window.closeWhenEmpty": true,
  "window.title": "${rootName}",
  "workbench.preferredLightColorTheme": "Red",
  "search.searchOnType": false,
  "editor.accessibilitySupport": "off",
  "tizen.workspace.path": "/Users/carlosvilla/workspace",
  "tizen.chrome.path": "Applications/Google Chrome.app/Contents/MacOS/Google Chrome",
  "hediet.vscode-drawio.local-storage": "",
  "codespaces.accountProvider": "Uninitialized",
  "workbench.panel.defaultLocation": "right",
  "workbench.editor.limit.enabled": true,
  "emmet.showSuggestionsAsSnippets": true,
  "angular.experimental-ivy": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "editor.minimap.enabled": false,
  "editor.renderControlCharacters": true,
  "vsicons.dontShowNewVersionMessage": true,
  "prettier.singleQuote": true,
  "prettier.semi": true,
  "prettier.vueIndentScriptAndStyle": true,
  "emmet.includeLanguages": {
    // "javascript": "javascriptreact"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "workbench.preferredHighContrastColorTheme": "Visual Studio Dark",
  "window.dialogStyle": "custom",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.startupEditor": "none",
  "window.nativeTabs": true,
  "window.titleBarStyle": "custom",
  "editor.cursorStyle": "line",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "editor.tabSize": 2,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": false
  },
  "window.zoomLevel": 1,
  "debug.javascript.autoAttachFilter": "onlyWithFlag"
}